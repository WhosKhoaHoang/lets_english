{

  "description": "Hopefully generates a gramatically correct English sentence from selected text.",
  "manifest_version": 2,
  "name": "Let's English!",
  "version": "1.0",
  "icons": {
    "48": "icons/lets_eng-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/lets_eng-48.png",
    "default_title": "Let's English!",
    "default_popup": "popup/lets_english.html"
  }
}
